											<xs:sequence>
												<xs:element minOccurs="0" name="ErcCode" type="SDScom:EnvironmentalReleaseCategoryCodeEnum"/>
												<xs:element minOccurs="0" name="ErcFulltext" type="SDScom:Phrase"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="SpecificEnvironmentalReleaseCategory">
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="SpercCode" type="SDScom:SpecificEnvironmentalReleaseCategoryCodeEnum"/>
												<xs:element minOccurs="0" name="SpercFulltext" type="SDScom:Phrase"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="ArticleCategoryNoIntendedRelease" type="SDScom:ArticleCategory"/>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="ArticleCategoryWithIntendedRelease" type="SDScom:ArticleCategory"/>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="CorrespondingExposureScenario" type="SDScom:CorrespondingEs"/>
