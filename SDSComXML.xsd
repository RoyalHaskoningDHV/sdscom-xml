						<xs:element maxOccurs="1" minOccurs="0" name="IsDangerReleasingSubstanceImdg" type="xs:boolean"/>
						<xs:element maxOccurs="1" minOccurs="0" name="IsDangerReleasingSubstanceIcao" type="xs:boolean"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute default="0" name="ComponentId" type="xs:integer"/>
		<xs:attribute default="" name="ComponentInternalSdsId" type="SDScom:string64"/>
	</xs:complexType>
	<!-- Section 4 ========================================================== -->
	<xs:complexType name="FirstAidMeasures">
		<xs:annotation>
			<xs:documentation>SDS section 4</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="DescriptionOfFirstAidMeasures">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Description of first aid measures: </xs:documentation>
				</xs:annotation>
