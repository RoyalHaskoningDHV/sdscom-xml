						<xs:element maxOccurs="unbounded" minOccurs="0" name="PhotochemicalOzoneCreationPotential" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="GlobalWarmingPotential" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="AtmosphericLifetime" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="EndocrineDisruptingPotential" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="DissolvedOrganicCarbon" type="SDScom:FurtherEcologicalData"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="TotalOrganicCarbon" type="SDScom:FurtherEcologicalData"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="InorganicCarbon" type="SDScom:FurtherEcologicalData"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="TheoreticalCarbonDioxideAmount" type="SDScom:FurtherEcologicalData"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="AOXAdsorbableOrganohalogens" type="SDScom:PhysChemUnitValue">
							<xs:annotation>
								<xs:documentation> Adsorbable organohalogens </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="POXPurgeableOrganohalogens" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="EOXExtractableOrganohalogens" type="SDScom:PhysChemUnitValue"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalEcologicalInformation" type="SDScom:Phrase"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Conclusion" type="SDScom:Phrase"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 13 ========================================================= -->
	<xs:complexType name="DisposalConsiderations">
		<xs:annotation>
			<xs:documentation>SDS section 13</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="WasteTreatment">
				<xs:annotation>
					<xs:documentation> Waste treatment: Specify the appropriate methods of disposal of both the chemical and any contaminated packaging. The actual use of the chemical will often influence the classification of the waste. Waste treatment containers and methods shall be specified including the appropriate methods of waste treatment of both the chemical and any contaminated packaging (e.g. incineration, recycling, landfilling). Physical or chemical properties that may affect waste treatment options shall be specified. Sewage disposal shall be discouraged. Where appropriate, any special precautions for any recommended waste treatment option shall be identified. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ProductWaste" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="PackagingWaste" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="EuRequirements">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> EU requirements: Information related to EU waste legislation. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="0" name="EuropeanWasteList">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> European waste list: EU classification of waste. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EWLProduct" type="SDScom:WasteListEntry">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> European waste list for the product: Identification number (EWL No.) and description of the waste under the actual EWL No. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EWLPacking" type="SDScom:WasteListEntry">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> European waste list for the packaging: Identification number (EWL No.) and description of the waste under the actual EWL No. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="EuWasteRegulations" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> EU legislation: EU legislation related to waste that applies to the product or its packaging. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Comments" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Comments on disposal considerations: Comments or information related to the information provided in this section. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 14 ========================================================= -->
	<xs:complexType name="TransportInformation">
		<xs:annotation>
			<xs:documentation>SDS section 14</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- (Not following EuphraC) -->
			<xs:element minOccurs="0" name="UnNo" type="SDScom:TransportUnNumbers">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> UN number: Identification number for products classified as dangerous goods. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="ProperShippingName" type="SDScom:TransportProperShippingNames">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Proper shipping name: Designation of the dangerous goods according to the transport regulations. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="TransportHazardClassification" type="SDScom:TransportHazardClassifications">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="PackingGroup" type="SDScom:TransportPackingGroups">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="EnvironmentalHazards" type="SDScom:TransportEnvironmentalHazards">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="SpecialPrecautionUser" type="SDScom:Phrase"/>
			<xs:element minOccurs="0" name="TransportInBulk">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="ProductName" type="SDScom:string128"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ShipType" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="PollutionCategory" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="OtherTransportInformation" type="SDScom:TransportOtherInformation">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Comments" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation>Any other information that applies to all transport carriers</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 15 ========================================================= -->
	<xs:complexType name="RegulatoryInfo">
		<xs:annotation>
			<xs:documentation>SDS section 15</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="SpecificProvisionsRelatedToProduct">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Specific regulations related to the chemical: If the chemical is subject of specific provisions in relation to protection of man or environment, these provisions shall be stated. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="EuLegislation">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> EU legislations: Restrictions and other regulatory requirements related to the chemical in accordance with EU legislations. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="AssessedLegislation" type="SDScom:AssessmentEnum"> </xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EuAuthorisation" type="SDScom:Phrase">
										<xs:annotation>
											<xs:appinfo>SDScomChem</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EuRestrictionsOnUse" type="SDScom:Phrase">
										<xs:annotation>
											<xs:appinfo>SDScomChem</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="RestrictionsAccordingReach" type="SDScom:Phrase">
										<xs:annotation>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> Restricted in use according REACH: Restrictions on the chemical according to Annex XVII of REACH </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Detergents">
										<xs:annotation>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> Restrictions related to regulation on detergents: </xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element maxOccurs="1" minOccurs="0" name="IsDetergent" type="xs:boolean"> </xs:element>
												<xs:element maxOccurs="unbounded" minOccurs="0" name="DetergentInformation" type="SDScom:Phrase"> </xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element minOccurs="0" name="Voc">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation>Restrictions related to the use of volatile organic compounds: Directive 2004/42/EC amending 1999/13/EC</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="VocInPercentByWeight" type="SDScom:PercentageValue">
													<xs:annotation>
														<xs:appinfo>SDScomBau</xs:appinfo>
														<xs:appinfo>SDScomChem</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element minOccurs="0" name="VocValue" type="SDScom:UnitValue">
													<xs:annotation>
														<xs:appinfo>SDScomBau</xs:appinfo>
														<xs:appinfo>SDScomChem</xs:appinfo>
														<xs:documentation>Voc contents: To be usually measured in g/L</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element maxOccurs="unbounded" minOccurs="0" name="VocRestrictionComments" type="SDScom:Phrase">
													<xs:annotation>
														<xs:appinfo>SDScomBau</xs:appinfo>
														<xs:appinfo>SDScomChem</xs:appinfo>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element minOccurs="0" name="IndustrialEmissions">
										<xs:annotation>
											<xs:documentation>Directive 2010/75/EU</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="VocInPercentByWeight" type="SDScom:PercentageValue"/>
												<xs:element minOccurs="0" name="VocValue" type="SDScom:UnitValue">
													<xs:annotation>
														<xs:documentation>Voc contents: To be usually measured in g/L</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element minOccurs="0" name="OtherInformation" type="SDScom:Phrase"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element minOccurs="0" name="CertainFluorinatedGreenhouseGases">
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="FluorinatedGreenhouseGasesMethod" type="SDScom:Phrase"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" name="FluorinatedGreenhouseGasesComments" type="SDScom:Phrase"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="OtherEuLegislation" type="SDScom:Phrase">
										<xs:annotation>
											<xs:documentation> Other EU legislations: Miscellaneous restrictions etc. that apply to the chemical based on EU legislations – e.g. regulation on construction products. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EuRestrictionsOfOccupation" type="SDScom:Phrase"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="NationalLegislation" type="SDScom:NationalRegulations">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> National legislation related to chemicals: National legislation can be defined as optional extensions prepared in national Xsd files, e.g. SdsXmlNE_DE.xsd for Germany. These files are included in the main SDSComXML.xsd validation file. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="RegulatoryInformationComments" type="SDScom:Phrase"/>
			<xs:element minOccurs="0" name="ChemicalSafetyAssessmentInfo">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="ChemicalSafetyAssessmentCarriedOut" type="xs:boolean">
							<xs:annotation>
								<xs:documentation> Chemical safety assessment (CSA) carried out: To state if CSA has been performed on the substance or the individual substances of a mixture. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ChemicalSafetyAssessment" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="ChemicalSafetyReport">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="CsrRequired" type="xs:boolean"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="CsrLocation" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="ExposureScenario">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="MixtureExposureScenarioInAnnex" type="xs:boolean">
							<xs:annotation>
								<xs:documentation> Exposure scenarios for the mixture: To state if information on the actual use of the mixture, similar to information in an exposure scenario for a substance, is enclosed the SDS. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ExposureScenarioComments" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Comments to exposure scenarioes: To provide information on status of ES, ES not required etc. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="RegulatoryInfoAdditionalInfo" type="SDScom:Phrase"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 16 ========================================================= -->
	<xs:complexType name="OtherInformation">
		<xs:annotation>
			<xs:documentation>SDS section 16</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ManufacturersNotes" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Manufacturer’s or supplier’s notes: Information </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RelevantHazardStatements" type="SDScom:HazardStatement" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Relevant hazard statements in full text: Write out the full text of any hazard statements referred to under section 2 and 3 of the SDS, with code numbers. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EducationalRecommendations" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Educational recommendations: Training advice </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecommendedRestriction" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Recommended restrictions: Information from supplier on recommended restrictions on the chemical. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FurtherInformation" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Further information: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MainInformationSource" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Main information sources: References to information sources applied in the compilation of the SDS. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="AbbreviationAcronymList">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Acronym" type="string"/>
						<xs:element name="Meaning" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AbbreviationsAndAcronymsUsed" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Abbreviations and acronyms: Explanation of terms used in the SDS must be part of the SDS. This field should cover (additional) links to external sources for such lists, e.g. EChA Guidance or industry specific glossaries.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RevisionInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation> Revision information: Information on changes in the SDS. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RevisionNo" type="SDScom:string16" minOccurs="0"/>
						<xs:element minOccurs="0" name="RevisionJustification" type="SDScom:JustificationForUpdate"/>
						<xs:element name="RevisionComments" type="SDScom:Phrase" minOccurs="0"/>
						<xs:element name="RevisionResponsible" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="LastUpdateDate" type="xs:date" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="QualityAssuranceStatement" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SdsStatus" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ResponsibleOwnerSds" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SdsPreparedBy" minOccurs="0">
				<xs:annotation>
					<xs:documentation> SDS preparer: </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Name" type="SDScom:string512" minOccurs="0"/>
						<xs:element name="Phone" type="SDScom:string40" minOccurs="0"/>
						<xs:element name="Email" type="SDScom:string256" minOccurs="0" maxOccurs="4"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Comments" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Comments to information in this section: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PositiveEcolabelling" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Positive ecolabel: For example the flower used in the EU. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="EcoLabelName" type="SDScom:string64" minOccurs="0"/>
						<xs:element name="ExpirationDate" type="xs:date" minOccurs="0"/>
						<xs:element name="LicenceNo" type="SDScom:string64" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ContentsOrIndexOfAnnexedEs" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Contents or index of exposure scenarioes (ES) in annex: To provide information on ES for components or corresponding information on the mixture supplied in annex(es) to the SDS. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Summary: The complete SDS ========================================== -->
	<xs:complexType name="Datasheet">
		<xs:sequence>
			<!-- *******************************************************************  SECTION 0 (Header informations) ************************  -->
			<xs:element name="InformationFromExportingSystem" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="XmlStandardVersionNo" type="SDScom:XmlStandardVersionEnum" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Version Number: The version of the SDSComXml standard which is applied in the transmission of information. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PhraseCatalogue" type="SDScom:PhraseCatalogue" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Phrase catalogue: The phrase library(ies) applied in the preparation of the SDS. The optional Xml attribute PhraseCatalogueId allows multiple catalogues to be used. Phrase must then have an Xml attribute specifying the PhraseCatalogueId. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Language" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> The language of the SDS, stated in a format according to ISO 639-1 (2 letters) followed by textual name </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FreetextLanguageCode" type="SDScom:LanguageCodeEnum" minOccurs="1" maxOccurs="1">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> Enumeration with defined code set. Language independent. ISO 639-1 (2 letters) </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FreetextLanguageName" type="SDScom:Phrase" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> String with actual language name. Possible to print. Automatic translation possible if a PhraseId is also given. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="RegulationsRelatedToCountryOrRegion" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Regulatory information related to country or region: What country the information in the SDS applies to, stated as a code in accordance to ISO 3166 (two letters). (A SDS may be written in English, and the information given may be in accordance to Norwegian regulations). </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RegulationsRelatedToCountryOrRegionCode" type="SDScom:CountryCodeEnum" minOccurs="1" maxOccurs="1">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> Enumeration with defined code set. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="RegulationsRelatedToCountryOrRegionName" type="SDScom:Phrase" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> String with actual country name. Possible to print. Automatic translation possible if a PhraseId is also given. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="SystemUsedInPreparation" type="SDScom:string64" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Software applied in preparation of information: State which chemicals information system the information has been exported from. The actual version number of the system applied must also be stated. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DateGeneratedExport" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Date for generating export: Date of generation of the export file with information according to the XML standard in the exporting system </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LegalDocument" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Legal documents: </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="LegalDocumentFileName" type="SDScom:string256" minOccurs="1" maxOccurs="1">
										<xs:annotation>
											<xs:appinfo>SDScomBau</xs:appinfo>
											<xs:appinfo>SDScomChem</xs:appinfo>
											<xs:documentation> File name: For example, a locked PDF document. A fixed URL is possible. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LegalDocumentPrintDate" type="xs:date" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:documentation> Print date </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LegalDocumentSignature" type="SDScom:string128" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:documentation> Digital signature: Digital signature to verify the document. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="SpecialExtensions" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation> Allows any additional values to be stored as a pairs of names and values together with a unique identity of the creator. This information can be ignored by receivers not making use of the special extensions, except that the data should be saved and passed on if the datasheet is transferred to others. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Extension" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ExtensionIssuerId" type="SDScom:string32" minOccurs="1" maxOccurs="1">
													<xs:annotation>
														<xs:documentation> Identifier of the company, country or other body creating the extension. A comapny is advised to use the ISO two letter country name followed by their entrprise registration number for uniqueness. Guids can be used for uniueness. </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ExtensionName" type="SDScom:string40" minOccurs="1" maxOccurs="1">
													<xs:annotation>
														<xs:documentation> A country unique name for any additional information. </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ExtensionValue" type="SDScom:string256" minOccurs="1" maxOccurs="1">
													<xs:annotation>
														<xs:documentation> The value of the additional information. </xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="RelatedDocuments" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation> Reference to further documents related to the SDS e.g. technical bulletin or manual. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FileName" type="SDScom:FileName">
										<xs:annotation>
											<xs:documentation> File name: For example, a locked PDF document. A fixed URL is possible. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Description" type="SDScom:string256" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:documentation> Description of the file type etc. </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="XMLStandardSubset" type="SDScom:XMLStandardSubsetEnum" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>SDScomBau</xs:appinfo>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Reference to the SDScomXML subsets SDScomBau and SDScomChem containing SDSComXML fields. More informaion to SDScomBau and SDScomChem are available at www.sdbtransfer.de </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- *******************************************************************  SECTION 1 (Identification of the substance/mixture and of the company/undertaking) ************************  -->
			<xs:element name="IdentificationSubstPrep" minOccurs="1" maxOccurs="1" type="SDScom:IdentificationSubstPrep">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 2 (Hazards identification) *******************************************************************************  -->
			<xs:element name="HazardIdentification" minOccurs="1" maxOccurs="1" type="SDScom:HazardIdentification">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Hazard identification: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 3 (Composition/information on ingredients) ***********************************************************  -->
			<xs:element name="Composition" minOccurs="1" maxOccurs="1" type="SDScom:Composition"> </xs:element>
			<!-- *******************************************************************  SECTION 4 (First aid measures) **********************************************************************************  -->
			<xs:element name="FirstAidMeasures" minOccurs="1" maxOccurs="1" type="SDScom:FirstAidMeasures">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> First aid measures: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 5 (Firefighting measures) ******************************************************************************  -->
			<xs:element name="FireFightingMeasures" minOccurs="1" maxOccurs="1" type="SDScom:FireFightingMeasures">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Fire fighting measures: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 6 (Accidental release measures) *********************************************************************  -->
			<xs:element name="AccidentalReleaseMeasures" minOccurs="1" maxOccurs="1" type="SDScom:AccidentalReleaseMeasures">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Accidental release measures: Information on response to spills - in cases where the spill volume has an impact on the hazard, distinguish between responses to large and small spills. If different practices for containment and recovery are required, this should be indicated. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 7 (Handling and storage) *****************************************************************************  -->
			<xs:element name="HandlingAndStorage" minOccurs="1" maxOccurs="1" type="SDScom:HandlingAndStorage">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Handling and storage: Advice on safe handling practices with emphasise on precautions that are appropriate to the identified uses referred to under subsection 1.2 and to the unique properties of the chemical. Information shall relate to the protection of human health, safety and the environment. It shall assist the employer in devising suitable working procedures and organisational measures according to Article 5 of Directive 98/24/EC and Article 5 of Directive 2004/37/EC. Where a chemical safety report is required, the information shall be consistent with the information given for the identified uses in the chemical safety report and the exposure scenarios showing control of risk. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 8 (Exposure controls / personal protection) ***************************************************************  -->
			<xs:element name="ExposureControlPersonalProtection" type="SDScom:ExposureControlPersonalProtection" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Exposure control and personal protection: This section shall describe the applicable occupational exposure limits and necessary risk management measures. Where a chemical safety report is required, the information in this section shall be consistent with the chemical safety report and the exposure scenarios showing control of risk from the chemical safety report set out in the annex to the safety data sheet. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 9 (Physical and chemical properties) ***************************************************************  -->
			<xs:element name="PhysicalChemicalProperties" type="SDScom:PhysicalChemicalProperties" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation>Physical and chemical properties: This section of the safety data sheet shall describe the empirical data relating to the substance or mixture, if relevant. In the case of a mixture, the entries shall clearly indicate to which substance in the mixture the data apply, unless it is valid for the whole mixture. The following properties shall be clearly identified including, where appropriate, a reference to the test methods used and specification of appropriate units of measurement and/or reference conditions. If relevant for the interpretation of the numerical value, the method of determination shall also be provided. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 10 (Stability and reactivity) ************************************************************************  -->
			<xs:element name="StabilityReactivity" minOccurs="1" maxOccurs="1" type="SDScom:StabilityReactivity">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Stability and reactivity: The section shall describe the stability of the chemical and the possibility of hazardous reactions occurring under certain conditions of use, also if released into the environment. Where appropriate, include a reference to the test methods used. If a particular property does not apply or if information is not available, the reasons shall be given. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 11 (Toxicological information) *********************************************************************  -->
			<xs:element name="ToxicologicalInformation" type="SDScom:ToxicologicalInformation" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 12 (Ecological information) *************************************************************************  -->
			<xs:element name="EcologicalInformation" type="SDScom:EcologicalInformation" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 13 (Disposal considerations) ***********************************************************************  -->
			<xs:element name="DisposalConsiderations" minOccurs="1" maxOccurs="1" type="SDScom:DisposalConsiderations">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Disposal considerations: The section shall describe information for proper waste management of the chemical and/or its container to assist in the determination of safe and environmentally preferred waste management options, consistent with the requirements in accordance with EU and national legislation. Information relevant for the safety of persons conducting waste management activities shall complement the information given in Section 8. The information on the waste management measures shall be consistent with the identified uses. If the disposal of the chemical presents a danger, a description of the residues and their safe handling shall be given. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 14 (Transport information) **************************************************************************  -->
			<xs:element name="TransportInformation" minOccurs="1" maxOccurs="1" type="SDScom:TransportInformation">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Transport information: This section shall provide basic classification information for transporting/shipment of chemicals mentioned under Section 1 by road, rail, sea, inland waterways or air. Where information is not available or relevant this shall be stated. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- ******************************************************************  SECTION 15 (Regulatory information) ************************************************************************  -->
			<xs:element name="RegulatoryInfo" minOccurs="1" maxOccurs="1" type="SDScom:RegulatoryInfo">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Regulatory information: This section shall describe regulatory information on the chemical that is not already provided in the safety data sheet (such as whether the substance or mixture is subject to Regulation (EC) No 2037/2000 of the European Parliament and of the Council of 29 June 2000 on substances that deplete the ozone layer, Regulation (EC) No 850/2004 of the European Parliament and of the Council of 29 April 2004 on persistent organic pollutants and amending Directive 79/117/EEC or Regulation (EC) No 689/2008 of the European Parliament and of the Council of 17 June 2008 concerning the export and import of dangerous chemicals). </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SECTION 16 (Other information) ****************************************************************************  -->
			<xs:element name="OtherInformation" minOccurs="1" maxOccurs="1" type="SDScom:OtherInformation">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Other information: The section shall describe the information relevant to the compilation of the safety data sheet. It shall incorporate other information that is not included in Sections 1 to 15, including information on revision of the safety data sheet. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- *******************************************************************  SDS MAIN BODY END ***********************************************************************************************  -->
			<xs:element maxOccurs="unbounded" minOccurs="0" name="SafeUseInfoForMixtures" type="SDScom:SUMI">
				<xs:annotation>
					<xs:documentation> Safe Use Information for Mixture (SUMI) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ExposureScenarioFile" type="SDScom:FileName">
				<xs:annotation>
					<xs:documentation> File containing exposure scenarios. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Root node for data exchange: DatasheetFeed ========================= -->
	<xs:element name="DatasheetFeed">
		<xs:annotation>
			<xs:documentation> The chemical safety datasheet using the included data types. To be filled in... </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<!-- *******************************************************************  Datasheet *************  -->
				<xs:element name="Datasheet" maxOccurs="unbounded" type="SDScom:Datasheet">
					<xs:annotation>
						<xs:documentation> The chemical safety datasheet using the included data types. To be filled in... </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
