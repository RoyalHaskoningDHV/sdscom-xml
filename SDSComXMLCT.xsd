				</xs:annotation>
			</xs:element>
			<xs:element name="TestDuration" type="UnitValue" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* DegradationElimination *************  -->
	<xs:complexType name="DegradationElimination">
		<xs:annotation>
			<xs:documentation> DegradationElimination, source: Chapter 12 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Value" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<!-- Was PhysChemUnitValue -->
			<!-- xs:element name="TestDuration" type="Phrase" minOccurs="0" maxOccurs="1"/ -->
			<xs:element name="Method" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Evaluation" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TestReference" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* MobilityData *************************  -->
	<xs:complexType name="MobilityData">
		<xs:annotation>
			<xs:documentation> MobilityData, source: Chapter 12. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Method" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TestReference" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* WasteListEntry ***********************  -->
	<xs:complexType name="WasteListEntry">
		<xs:annotation>
			<xs:documentation> WasteListEntry, source: Chapter 13. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="WasteCode" type="WasteCode" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="WasteDescription" type="Phrase" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="HazardousWaste" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation>optional because this property can be dreived from WasteCode</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* ProductGtin ***********************  -->
	<xs:complexType name="ProductGtin">
		<xs:annotation>
			<xs:documentation> ProductGtin </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="No" type="int20" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation> GTIN No: </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="string512" minOccurs="0" maxOccurs="1"> </xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* StorageStability *************  -->
	<xs:complexType name="StorageStability">
		<xs:sequence>
			<xs:element name="StorageTemperature" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Pressure" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MaximumStoragePeriod" type="UnitValue" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* Ecotoxicity *************  -->
	<xs:complexType name="Ecotoxicity">
		<xs:annotation>
			<xs:documentation> Ecotoxicity. Source: Chapter 12. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ToxicityType" type="ToxicityTypeEnum" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Value" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="EffectDoseConcentration" type="DoseEnum" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ExposureTime" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TestDuration" type="UnitValue" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Species" type="Species" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Method" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TestReference" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Evaluation" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *******************************************************************  GasGroup *********************************  -->
	<xs:complexType name="GasGroup">
		<xs:annotation>
			<xs:documentation> GasGroup. Source: Chapter 9. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Method" type="Phrase" minOccurs="0"/>
			<xs:element name="Source" type="Phrase" minOccurs="0"/>
			<xs:element name="TestResults" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Evaluation" type="GasGroupEnum" minOccurs="0"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *******************************************************************  Species *************  -->
	<xs:complexType name="Species">
		<xs:annotation>
			<xs:documentation> Species used in tests </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="Phrase"/>
			<xs:element name="Gender" type="Phrase" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *******************************************************************  TemperatureClass **************************  -->
	<xs:complexType name="TemperatureClass">
		<xs:annotation>
			<xs:documentation> TemperatureClass </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Method" type="Phrase" minOccurs="0"/>
			<xs:element name="Source" type="Phrase" minOccurs="0"/>
			<xs:element name="TestResults" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Evaluation" type="TemperatureClassEnum" minOccurs="0"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *****************************************************************  PhysicalHazardsGeneralInformation ***********  -->
	<xs:complexType name="PhysicalHazardsGeneralInformation">
		<xs:annotation>
			<xs:documentation> PhysicalHazardsGeneralInformation. Source: Chapter 9. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="JustificationForDataWaiving" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Assessment" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Classification" type="Phrase" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ****************************************************************************** PhraseCatalogue *****  -->
	<xs:complexType name="PhraseCatalogue">
		<xs:annotation>
			<xs:documentation> Catalogue of standard phrases used in the datasheet. Used in chapter 0 and referred in for example exposure scenarios. PhraseCatalogues and PhraseIds simplifies translation to other languages. Example: EUPhraC catalogue. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCatalogueName" type="string256" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation>Phrase catalogue Name, eg. EuPhraC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueIssuer" type="string256" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Phrase catalogue issuer: Identification of the suppler of the phrase catalogue to the exporting system, eg. EuPhraC-Workinggroup. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueVersion" type="string64" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Catalogue version: The version no of the phrase catalog the information is based on to identify the actual version of the phrase catalogue applied. Phrase catalogues are updated more or less regularly, and different versions may show some differences. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueDate" type="xs:date">
				<xs:annotation>
					<xs:appinfo>SDScomBau</xs:appinfo>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation>Date of release of the catalogue version: The version timestamp of the phrase catalogue the information is based on</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ****************************************************************************** SUMI = Safe Use Information for Mixtures ***** -->
	<xs:complexType name="SUMI">
		<xs:annotation>
			<xs:documentation>Safe Use Information for Mixtures. Based on a draft prepared by Qualisys GmbH, Langenfeld, Germany, and published under CC license BY-SA 4.0 during the BDI workshop in Berlin, 26 October 2016. Includes information from the presentation of Martin Kops at ENES 8 and other documents.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="SumiReference">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="SectorSumiCode" type="xs:string">
							<xs:annotation>
								<xs:documentation>Leave empty if the Sumi is not published by a chemical sector association and part of Use Map activities (no company specific SUMI codes here!). First word should be the sector name, e.g. AISE or FEICA, followed by the sector's SUMI code. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="VersionNumber" type="string16">
							<xs:annotation>
								<xs:documentation>Version number is optional here because SUMI info is part of the eSDS which is versioned, so the version date is expected to be the same and is sufficient to determine a sector SUMI version.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" name="Title" type="Phrase"> </xs:element>
			<xs:element name="GeneralDescription">
				<xs:complexType>
					<xs:choice>
						<xs:element name="UseDescriptors" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="SectorOfUse" type="SectorOfUse"/>
									<xs:element minOccurs="0" name="ProcessCategory" type="ProcessCategory"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="SwedReference" type="Phrase">
							<xs:annotation>
								<xs:documentation>Reference to the Sector-specific Worker Exposure Description as named in the sector's Use Map.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" name="Text" type="Phrase"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="OperationalConditions">
				<xs:complexType>
					<xs:choice>
						<xs:element name="Properties">
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="MaxDuration" type="OneUnitValue">
										<xs:annotation>
											<xs:documentation>Maximum duration of the use/application.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="RangeOfApplication" type="Phrase"/>
									<xs:element minOccurs="0" name="AirExchangeRate" type="OneUnitValue">
										<xs:annotation>
											<xs:documentation>Air exchange rate (equals VentilationRate in ESCom XML) is a minimum value.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="Other" type="Phrase"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" name="Text" type="Phrase"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="RiskManagementMeasures">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="RmmWorker" type="Phrase"/>
						<xs:element name="RmmPictogram" type="RmmPictogramEnum" maxOccurs="unbounded" minOccurs="0"> </xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="RmmEnvironment" type="Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="GoodPracticeAdvice">
				<xs:annotation>
					<xs:documentation>In Exposure Scenarios, also known as AGPA = Additional Good Practice Advice</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Text" type="Phrase"/>
						<xs:element minOccurs="0" name="AgpaPictogram" type="AgpaPictogramEnum"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalInfo" type="Phrase"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="DisclaimerBoundaries" type="Phrase">
				<xs:annotation>
					<xs:documentation>Describes the boundaries of the SUMI. Mandatory according to some sources, optional according to others.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
