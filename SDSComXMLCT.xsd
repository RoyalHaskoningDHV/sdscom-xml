		<xs:sequence>
			<xs:element name="Group" type="EffectLevelGroupEnum" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ExposureRouteFrequencyAndEffect" type="EffectLevelTypeEnum" minOccurs="1" maxOccurs="1">
